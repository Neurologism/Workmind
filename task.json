{
  "operations": [
    {
        "type": "dataset",
        "method": "new",
        "uid": "dataset1",
        "args": {
            "class": "mnist",
            "split": "train",
            "preprocess": [
              "normalize"
            ],
            "batch_size": 32
        }
    },
    {
      "type": "layer",
      "method": "new",
      "uid": "input1",
      "args": {
          "class": "Input",
          "shape": [
            28,
            28
          ]
        }
    },

    {
        "type": "layer",
        "method": "new",
        "uid": "flatten1",
        "args": {
            "class": "Flatten",
            "inputs": "input1"
        }
    },

    {
      "type": "layer",
      "method": "new",
      "uid": "layer1",
      "args": {
        "class": "Dense",
        "units": 10,
        "activation": {
          "method": "softmax"
        },
        "inputs": "flatten1"
      }
    },
    {
      "type": "model",
      "method": "new",
      "uid": "model1",
      "args": {
        "inputs": "input1",
        "outputs": "layer1"
      }
    },
    {
      "type": "model",
      "method": "compile",
      "uid": "model1",
      "args": {
        "optimizer": {
          "method": "adam"
        },
        "loss": [
          {
            "method": "sparse_categorical_crossentropy"
          }
        ],
        "metrics": [
          {
            "method": "sparse_categorical_accuracy"
          }
        ]
      }
    },
    {
      "type": "model",
      "method": "fit",
      "uid": "model1",
      "args": {
        "dataset": "dataset1",
        "epochs": 1
      }
    }
  ]
}